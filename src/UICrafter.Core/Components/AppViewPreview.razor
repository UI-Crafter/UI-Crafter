@using UICrafter.Core.Components.Render
@* Extract from Mobile: AppViewPage *@
<CascadingValue Value="jsonPublisher">
    <CascadingValue Value="inputFieldPublisher">
        <ComponentListRender UIComponents="UIComponents" />
    </CascadingValue>
</CascadingValue>


@code {
    [Parameter]
    public required ObservableCollection<UIComponent> UIComponents { get; set; }

    private JsonPublisher jsonPublisher = new JsonPublisher();
    private InputFieldPublisher inputFieldPublisher = new InputFieldPublisher();
}
