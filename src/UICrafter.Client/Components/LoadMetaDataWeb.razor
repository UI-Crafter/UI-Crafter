@inherits BaseLoadMetaDataList

@{
    base.BuildRenderTree(__builder);
}

@code {
    [Parameter]
    public EventCallback<long> ClickFunction { get; set; }

    protected override void HandleClick(AppView view)
    {
        if (ClickFunction.HasDelegate)
        {
            ClickFunction.InvokeAsync(view.Id);
        }
    }
}
