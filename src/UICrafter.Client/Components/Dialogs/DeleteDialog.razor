<MudDialog Class="">
    <DialogContent>
        <MudText>Are you sure you want to delete this app-view?</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="() => MudDialog.Close(DialogResult.Ok(true))">Yes</MudButton>
        <MudButton OnClick="() => MudDialog.Cancel()">No</MudButton>
    </DialogActions>
</MudDialog>


@code {
    [CascadingParameter]
    private MudDialogInstance MudDialog { get; set; } = null!;

    protected override async Task OnInitializedAsync()
    {
        await base.OnInitializedAsync();
        MudDialog.Options.NoHeader = true;
    }
}
